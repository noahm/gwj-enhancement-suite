{
    "manifest_version": 2,
    "name": "GWJ Enhancement Suite",
    "description": "Provides extra features for gamerswithjobs.com",
    "author": "Noah \"Cathadan\" Manneschmidt",
    "version": "1.0.0",
    "content_scripts": [
        {
            "matches": ["https://www.gamerswithjobs.com/*"],
            "js": ["content_scripts/index.js"]
        }
    ],
    "options_ui": {
        "page": "settings/settings.html"
    },
    "permissions": ["storage"]
}
